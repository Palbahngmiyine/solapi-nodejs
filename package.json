{
  "name": "solapi",
  "version": "5.5.0",
  "description": "SOLAPI SDK for Node.js(Server Side Only)",
  "keywords": [
    "solapi",
    "sms",
    "lms",
    "mms",
    "kakao alimtalk",
    "sdk",
    "korean sms"
  ],
  "homepage": "https://solapi.com",
  "bugs": {
    "url": "https://github.com/solapi/solapi-nodejs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solapi/solapi-nodejs.git"
  },
  "license": "MIT",
  "author": {
    "name": "SOLAPI Team",
    "email": "contact@solapi.com"
  },
  "type": "commonjs",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "yarn lint && tsup-node src/index.ts",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "format": "prettier ./**/*.ts ./**/*.js ./**/*.mjs",
    "lint": "eslint .",
    "dev": "tsup src/index.ts --watch"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "effect": "^3.16.5",
    "zod": "^3.25.57"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.9.2",
  "engines": {
    "node": ">=18"
  }
}
